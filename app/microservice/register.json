{
    "name": "#(service.name)",
    "endpoints": [{
        "path": "/metadata/:dataset",
        "method": "GET",
        "redirect": {
            "method": "GET",
            "path": "/metadata/:dataset"
        }
    }, {
        "path": "/metadata/:dataset/:app",
        "method": "GET",
        "redirect": {
            "method": "GET",
            "path": "/metadata/:dataset/:app"
        }
    }, {
        "path": "/metadata/find-by-ids",
        "method": "POST",
        "redirect": {
            "method": "POST",
            "path": "/metadata/find-by-ids"
        }
    }, {
        "path": "/metadata/:dataset/:app",
        "method": "POST",
        "redirect": {
            "method": "POST",
            "path": "/metadata/:dataset/:app"
        }
    }, {
        "path": "/metadata/:dataset/:app",
        "method": "DELETE",
        "redirect": {
            "method": "DELETE",
            "path": "/metadata/:dataset/:app"
        }
    }, {
        "path": "/metadata/:dataset/:app",
        "method": "PATCH",
        "redirect": {
            "method": "PATCH",
            "path": "/metadata/:dataset/:app"
        }
    }]
}
